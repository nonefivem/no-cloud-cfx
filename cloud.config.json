{
  "$schema": "./config.schema.json",
  "client_identifier_extractor": "ip:license",
  "logging": {
    "enabled": true,
    "level": "info"
  },
  "storage": {
    "enable_client_uploads": true,
    "max_file_size_mb": 50,
    "allowed_file_types": [
      "text/plain",
      "image/jpeg",
      "image/png",
      "image/gif",
      "image/webp",
      "image/svg+xml",
      "video/mp4",
      "video/webm",
      "audio/mpeg",
      "audio/wav"
    ],
    "metadata_attachments": {
      "resource": true,
      "player": true
    },
    "rate_limit": {
      "enabled": true,
      "window_ms": 60000,
      "max_requests": 15
    }
  }
}
