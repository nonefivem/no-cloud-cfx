{
  "$schema": "./config.schema.json",
  "logging": {
    "enabled": true,
    "level": "info"
  },
  "storage": {
    "allowed_file_types": ["image/png", "image/jpeg", "image/gif"],
    "max_file_size_mb": 50,
    "rate_limit": {
      "enabled": true,
      "client_uploads_per_minute": 20,
      "client_identifier_extractor": "ip:license"
    }
  }
}
